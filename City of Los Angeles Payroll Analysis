     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "DROP TABLE IF EXISTS payroll_dept;\n",
    "\n",
    "CREATE TABLE payroll_dept(\n",
    "    dept_key VARCHAR PRIMARY KEY NOT NULL,\n",
    "    dept_title VARCHAR\n",
    ")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
