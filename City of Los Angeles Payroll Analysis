    {
     "data": {
      "text/plain": [
       "[]"
      ]
     },
     "execution_count": 46,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "INSERT INTO budget (dept_name)\n",
    "SELECT dept_title\n",
    "FROM payroll_dept AS p\n",
    "WHERE NOT EXISTS(\n",
    "  SELECT dept_name\n",
    "  FROM budget\n",
    "  WHERE dept_name = p.dept_title\n",
    ")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "#### Build a table of each department's total budget from 2013 to 2018"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
