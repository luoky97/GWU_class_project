     "execution_count": 49,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "ALTER TABLE payroll_dept\n",
    "ADD FOREIGN KEY (dept_title) REFERENCES budget_sum(dept_name)"
   ]
