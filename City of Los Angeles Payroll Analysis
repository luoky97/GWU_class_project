      "Done.\n",
      "Done.\n"
     ]
    },
    {
     "data": {
      "text/plain": [
       "[]"
      ]
     },
     "execution_count": 57,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "DROP TABLE IF EXISTS sal_budg;\n",
    "CREATE TABLE sal_budg(\n",
    "    dept_title VARCHAR,\n",
    "    salary_budget NUMERIC,\n",
    "    other_budget NUMERIC\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 58,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      " * postgresql://student@/final_proj\n",
      "48 rows affected.\n"
     ]
    },
    {
     "data": {
      "text/plain": [
       "[]"
      ]
     },
     "execution_count": 58,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "INSERT INTO sal_budg\n",
    "SELECT b.dept_name, SUM(s.projected_annual_sal), (b.budget_sum-SUM(s.projected_annual_sal))\n",
    "FROM (\n",
    "    SELECT sf.*, p.dept_title\n",
    "    FROM salary_fact AS sf\n",
    "    JOIN payroll_dept AS p\n",
    "    ON sf.dept_key = p.dept_key\n",
    ")s\n",
    "JOIN budget_sum AS b\n",
    "ON s.dept_title = b.dept_name\n",
    "GROUP BY b.dept_name;"
   ]
  },
