    "INSERT INTO healthcare_fact\n",
    "SELECT r.row_id, r.year, r.dept_key, r.job_class_id, r.emp_type, r.avg_health_cost,\n",
    "       r.avg_dental_cost, r.avg_basic_life, r.avg_bf_cost\n",
    "FROM raw AS r"
