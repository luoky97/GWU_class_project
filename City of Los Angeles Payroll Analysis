    "%%sql\n",
    "SELECT p.dept_title, AVG(h.avg_health_cost) AS \"Average Health Cost\"\n",
    "FROM healthcare_fact AS h \n",
    "JOIN payroll_dept AS p\n",
    "ON p.dept_key = h.dept_key\n",
    "GROUP BY p.dept_title\n",
    "ORDER BY AVG(h.avg_health_cost)\n",
    "LIMIT 5"
   ]
