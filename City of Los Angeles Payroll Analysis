     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "DROP TABLE IF EXISTS budget_sum;\n",
    "CREATE TABLE budget_sum(\n",
    "    dept_name VARCHAR PRIMARY KEY,\n",
    "    budget_sum NUMERIC\n",
    ");"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      " * postgresql://student@/final_proj\n",
      "48 rows affected.\n"
     ]
    },
    {
     "data": {
      "text/plain": [
       "[]"
      ]
     },
     "execution_count": 48,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "%%sql\n",
    "INSERT INTO budget_sum\n",
    "SELECT dept_name, SUM (total_budget) AS total_budget\n",
    "FROM budget \n",
    "GROUP BY dept_name;"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "metadata": {},
   "outputs": [
