   "source": [
    "%%sql\n",
    "DROP TABLE IF EXISTS raw;\n",
    "\n",
    "CREATE TABLE raw (\n",
    "    row_id VARCHAR,\n",
    "    year VARCHAR,\n",
    "    dept_title VARCHAR,\n",
    "    dept_key VARCHAR,\n",
    "    job_class_title VARCHAR,\n",
    "    emp_type VARCHAR,\n",
    "    hourly_rate NUMERIC,\n",
    "    projected_annual_sal NUMERIC,\n",
    "    total_pay NUMERIC,\n",
    "    base_pay NUMERIC,\n",
    "    perm_bonus NUMERIC,\n",
    "    long_bonus NUMERIC,\n",
    "    temp_bonus NUMERIC,\n",
    "    lump_sum NUMERIC,\n",
    "    ot_pay NUMERIC,\n",
    "    other_n_adj NUMERIC,\n",
    "    other_sum NUMERIC,\n",
    "    job_class_id VARCHAR,\n",
    "    avg_health_cost NUMERIC,\n",
    "    avg_dental_cost NUMERIC,\n",
    "    avg_basic_life NUMERIC,\n",
    "    avg_bf_cost NUMERIC\n",
    ")"
   ]
